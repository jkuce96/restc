        <% imageData.slice(0, 8).forEach(image => { %>
            <li class="relative">
                <div class="overflow-hidden rounded-lg shadow-lg">
                    <a href="<%= image.permalink %>" target="_blank" class="block">
                        <img src="<%= image.media_url %>" alt="<%= image.caption %>"
                             class="w-full h-full transition duration-300 transform hover:scale-105">
                        <div class="absolute top-0 left-0 w-full h-full bg-white opacity-0 hover:opacity-75 flex items-center justify-center text-black text-opacity-100 hover:text-opacity-100 transition duration-300">
                            <p class="text-center p-4"><%= image.caption %></p>
                        </div>
                    </a>
                </div>
            </li>
        <% }); %>
    </ul>
</section>


