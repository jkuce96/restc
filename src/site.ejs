    <style>
		.custom-underline {
			position: relative;
		}

		.custom-underline::after {
			content: '';
			position: absolute;
			left: 0;
			bottom: -2px;
			width: 100%;
			height: 2px;
			background: currentColor;
			transform: scaleX(0);
			transition: transform 0.3s ease-in-out;
			transform-origin: center;
		}

		.custom-underline:hover::after,
		.custom-underline:focus::after {
			transform: scaleX(1);
		}

		.custom-underline:hover,
		.custom-underline:focus {
			color: #585858;
		}

		.custom-underline-footer:hover,
		.custom-underline-footer:focus {
			color: #FFF;
		}

		svg:hover {
			fill: #C9C9C9;
			transform: scale(1.1);
		}

		#scrollTopBtn svg:hover {
			fill: #C9C9C9;
			transform: rotate(-19deg);
		}

    </style>
